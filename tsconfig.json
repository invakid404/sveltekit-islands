{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules/**/*"],
	"extends": ["./.svelte-kit/tsconfig.json"]
}
